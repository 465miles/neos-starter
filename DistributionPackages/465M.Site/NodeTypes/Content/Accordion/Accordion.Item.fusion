prototype(465M.Site:Content.Accordion.Item) < prototype(Neos.Neos:ContentComponent) {
    id = ${node.identifier}
    ref = ${'accordionitem-' + this.id}

    title = Neos.Neos:Editable {
        property = 'title'
    }

    content = Neos.Neos:ContentCollection {
        nodePath = ''
        @process.contentElementWrapping >
    }

    renderer = afx`
        <465M.Site:Component.Atom.AccordionItem id={props.id} ref={props.ref} title={props.title} content={props.content} />
    `
}
