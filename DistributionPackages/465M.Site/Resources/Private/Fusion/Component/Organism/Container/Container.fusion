prototype(465M.Site:Component.Organism.Container) < prototype(Neos.Fusion:Component) {

    @propTypes {
        @strict = true
        content = ${PropTypes.any}
        extraClass = ${PropTypes.string}
    }

    renderer = afx`
        <div class.main="w-full max-w-7xl mx-auto px-5 md:px-10" class.extra={props.extraClass}>
            {props.content}
        </div>
    `
}
