prototype(465M.Site:Document.Page) < prototype(Neos.Neos:ContentComponent) {

    title = ${q(node).property('title')}

    content = Neos.Neos:ContentCollection {
        nodePath = 'main'
    }

    @process.contentElementWrapping >

    renderer = afx`
        <465M.Site:Component.Page.Document title={props.title} content={props.content} />
    `
}
